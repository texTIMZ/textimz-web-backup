{
  "name":              "dreamfactory/dreamfactory",
  "description":       "The DreamFactory(tm) Platform.",
  "keywords":          [
    "api",
    "dreamfactory",
    "laravel",
    "rest"
  ],
  "homepage":          "https://www.dreamfactory.com/",
  "license":           "Apache-2.0",
  "type":              "project",
  "authors":           [
    {
      "name":  "Arif Islam",
      "email": "arifislam@dreamfactory.com"
    },
    {
      "name":  "Charles Harmon",
      "email": "charlesharmon@dreamfactory.com"
    },
    {
      "name":  "Lee Hicks",
      "email": "leehicks@dreamfactory.com"
    }
  ],
  "support":           {
    "email":  "support@dreamfactory.com",
    "source": "https://github.com/dreamfactorysoftware/dreamfactory",
    "issues": "https://github.com/dreamfactorysoftware/dreamfactory/issues",
    "wiki":   "https://wiki.dreamfactory.com"
  },
  "minimum-stability": "dev",
  "prefer-stable":     true,
  "require":           {
    "dreamfactory/df-admin-app":       "~2.7.0",
    "dreamfactory/df-apidoc":          "~0.1.0",
    "dreamfactory/df-aws":             "~0.9.0",
    "dreamfactory/df-azure":           "~0.9.0",
    "dreamfactory/df-cache":           "~0.6.0",
    "dreamfactory/df-cassandra":       "~0.6.0",
    "dreamfactory/df-core":            "~0.9.0",
    "dreamfactory/df-couchbase":       "~0.4.0",
    "dreamfactory/df-couchdb":         "~0.9.0",
    "dreamfactory/df-email":           "~0.3.0",
    "dreamfactory/df-file":            "~0.1.0",
    "dreamfactory/df-filemanager-app": "~0.1.0",
    "dreamfactory/df-firebird":        "~0.1.0",
    "dreamfactory/df-mongodb":         "~0.9.0",
    "dreamfactory/df-oauth":           "~0.9.0",
    "dreamfactory/df-rackspace":       "~0.9.0",
    "dreamfactory/df-rws":             "~0.9.0",
    "dreamfactory/df-script":          "~0.3.0",
    "dreamfactory/df-sqldb":           "~0.9.0",
    "dreamfactory/df-swagger-ui":      "~0.2.0",
    "dreamfactory/df-user":            "~0.9.0",
    "laravel/framework":               "5.4.*",
    "predis/predis":                   "~1.0",
    "tymon/jwt-auth":                  "~1.0.0"
  },
  "require-dev":       {
    "barryvdh/laravel-ide-helper": "~2.1",
    "fzaninotto/faker":            "~1.4",
    "laracasts/generators":        "~1.0",
    "laracasts/testdummy":         "~2.0",
    "laravel/homestead":           "^4.0",
    "mockery/mockery":             "0.9.*",
    "phpunit/phpunit":             "~5.0"
  },
  "autoload":          {
    "classmap": [
      "database"
    ],
    "psr-4":    {
      "DreamFactory\\": "app/"
    }
  },
  "autoload-dev":      {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts":           {
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan clear-compiled",
      "php artisan optimize",
      "php artisan df:homestead-config"
    ],
    "post-update-cmd":  [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan clear-compiled",
      "php artisan optimize",
      "php artisan df:homestead-config"
    ]
  },
  "config":            {
    "preferred-install": "dist",
    "sort-packages":     true
  },
  "extra":             {
    "branch-alias":    {
      "dev-develop": "2.6.x-dev"
    },
    "installer-paths": {
      "public/{$name}/": [
        "type:dreamfactory-app"
      ]
    }
  }
}